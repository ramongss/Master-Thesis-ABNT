@article{Bozek2017,
abstract = {Models and optimisation approaches are developed for a flexible job shop scheduling problem with lot streaming and lot sizing of the variable sublots.Atwo-stage optimisation procedure is proposed. First, the makespan value is minimised with the smallest sublots defined for the problem instance. This makes it possible to shorten the makespan significantly, because each sublot is transferred separately to the next operation of a job. In the second stage, the sizes of the sublots are maximised without increasing the obtainedmakespan value. In this way, the quantity of sublots and transport activities is limited together with the related manufacturing cost. Two objectives are defined for the second stage. The first one is themaximisation of the sum of the sublot sizes of all operations, the second one is the maximisation of the number of the operations which do not need to be split at all. Mixed-integer linear programming, constraint programming and graph-based models are implemented for the problem. Two optimisation approaches are developed and compared in computational experiments for each stage and objective, one approach is based on a third-party solver, and the second one on an independent own implementation, namely a tabu search and a greedy constructive heuristic. Keywords:},
author = {Bo{\.{z}}ek, Andrzej and Werner, Frank},
doi = {10.1080/00207543.2017.1346322},
file = {:C$\backslash$:/Users/RAMON/Dropbox/PUCPR/Mestrado/Estudos Independentes/Material/Artigos/Lot Streaming/boek2017.pdf:pdf},
issn = {0020-7543},
journal = {International Journal of Production Research},
keywords = {constraint programming,flexible job shop,lot sizing,lot streaming,mixed-integer linear programming,tabu search},
month = {jul},
pages = {1--21},
title = {{Flexible job shop scheduling with lot streaming and sublot size optimisation}},
url = {https://www.tandfonline.com/doi/full/10.1080/00207543.2017.1346322},
year = {2017}
}
