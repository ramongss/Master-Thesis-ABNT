@phdthesis{CONTE2016,
abstract = {The objective of this work is to develop a mathematical model to represent the con- cept of clearing function (CF), which regulates the throughput rate in a production system as a function of the system's workload. A new approach to Little's law is used, suggesting that a system's throughput rate depends on two variables: system load and cycle time. The clearing function concept allows throughput rates to be defined in order to feed the linear programming models involved in mathematical modelling of the production systems. The main approach used to achieve these goals has been to combine linear programming models with the concept of clearing func- tion. Nevertheless, at least as of this writing the results have not been as remarkable as expected. This present study argues that a major reason for these disappointing results is the way previous approaches have considered the clearing function. The assumption that system loading is the only argument for clearing function is not sufficient to obtain a model that can provide all the needed information about th- roughput rates and cycle times in the system. One of the issues discussed is whether a clearing function which depends on two variables (system load and cycle time) is a powerful model for representing this throughput rate; in other words, the goal here is how to efficiently model the clearing function as a function of two variables, and to compare this new approach with the approach to clearing function that depends on system loading, which has hitherto been used in the literature. The experimental design consisted of generating values for the response variable X (throughput rate) as a function of two other variables, a fixedW (work in process) and another random TC (cycle time in the system). The dynamic regression model for CF emerged from the implementation of the new methodology, which initially involved analyzing the normality of the residuals obtained from adjusting the set of non-linear regression models and then adjusting the temporal series in the residuals which had rejected the normality hypothesis. In the first stage, we used the Monte Carlo simulation with 500 repetitions to collect data and to estimate and evaluate normality. At this stage, the two-variable model yielded a higher number of repetitions with normal distribution for the residuals than the functional model used in the literature. In the second stage, parameters were estimated using time series for the abnormal residues and the white noise was analyzed. Both models adjusted to the residuals for the time series were successful in all repetitions.},
author = {Conte, Viviane Cristhyne Bini Conte},
file = {:C$\backslash$:/Users/RAMON/Dropbox/PUCPR/Mestrado/Estudos Independentes/Material/Grupo de Pesquisa PO/Disserta{\c{c}}{\~{o}}es e Teses/Tese Viviane.pdf:pdf},
keywords = {Clearing Function,Linear Programming,Production Planning,Th- roughput Rate,Work in Process.},
pages = {122},
school = {Pontif{\'{i}}cia Universidade Cat{\'{o}}lica do Paran{\'{a}}},
title = {{Um modelo de regress{\~{a}}o din{\^{a}}mica para a fun{\c{c}}{\~{a}}o clearing function que estima a taxa de produ{\c{c}}{\~{a}}o em modelos de planejamento de produ{\c{c}}{\~{a}}o baseados em programa{\c{c}}{\~{a}}o matem{\'{a}}tica}},
year = {2016}
}
